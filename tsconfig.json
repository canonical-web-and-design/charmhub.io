{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020", // default value is ES3
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "isolatedModules": true,
    "noImplicitAny": false,
    "noEmit": true
  },
  "include": ["static/js/src"]
}
